    syntax on
    "colorscheme molokai
    set t_Co=256
    
    set encoding=utf-8
    set fileencoding=utf-8
    set fileencodings=iso-2022-jp,euc-jp,utf-8,ucs-2,cp932,sjis
    "set autoindent
    set nobackup
    set number
    set title
    set ambiwidth=double
    set tabstop=4
    set expandtab
    set shiftwidth=4
    set cindent
    set smartindent
    set list
    set listchars=tab:»-,trail:-,eol:↲,extends:»,precedes:«,nbsp:%
    set nrformats-=octal
    set hidden
    set history=50
    set virtualedit=block
    set backspace=indent,eol,start
    set wildmenu
    set nocompatible
    set whichwrap=b,s,[,],<,>
    set hlsearch
    set showmatch
    set showcmd
    highlight ZenkakuSpace cterm=underline ctermfg=lightblue guibg=darkgray
    match ZenkakuSpace /＿/
